    "fix": "node dist/index.js fix",
    "lint:ts": "node dist/index.js lint.ts",
    "test[0]": "node dist/index.js test[0]"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/clean-scripts.git"
  },
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/clean-scripts/issues"
  },
  "typeCoverage": {
    "atLeast": 97
  },
  "homepage": "https://github.com/plantain-00/clean-scripts#readme",
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@types/jasmine": "3.5.4",
    "@types/minimist": "1.2.0",
    "@types/node": "13.7.1",
    "@types/pidusage": "2.0.1",
    "@typescript-eslint/eslint-plugin": "2.19.2",
    "@typescript-eslint/parser": "2.19.2",
    "clean-release": "2.7.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-plantain": "0.1.2",
    "jasmine": "3.5.0",
    "markdownlint-cli": "0.22.0",
    "no-unused-export": "1.10.0",
    "rimraf": "3.0.2",
    "type-coverage": "2.4.0",
    "typescript": "3.7.5"
  },
  "dependencies": {
    "minimist": "1.2.0",
    "pidusage": "^2.0.17",
    "pretty-ms": "6.0.0",
    "tslib": "1"
  },
  "bin": {
    "clean-scripts": "bin/clean-scripts"
