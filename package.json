{
  "name": "clean-scripts",
  "version": "1.12.3",
  "description": "A CLI tool to make scripts in package.json clean.",
  "main": "dist/core.js",
  "types": "dist/core.d.ts",
  "scripts": {
    "build": "node dist/index.js build --config clean-scripts.config.ts",
    "lint": "node dist/index.js lint --config clean-scripts.config.ts",
    "test": "node dist/index.js test --config clean-scripts.config.ts",
    "fix": "node dist/index.js fix --config clean-scripts.config.ts",
    "lint:ts": "node dist/index.js lint.ts --config clean-scripts.config.ts",
    "test[0]": "node dist/index.js test[0] --config clean-scripts.config.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/clean-scripts.git"
  },
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/clean-scripts/issues"
  },
  "typeCoverage": {
    "atLeast": 97
  },
  "homepage": "https://github.com/plantain-00/clean-scripts#readme",
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@types/jasmine": "3.5.10",
    "@types/minimist": "1.2.0",
    "@types/node": "13.13.5",
    "@types/pidusage": "2.0.1",
    "@typescript-eslint/eslint-plugin": "2.31.0",
    "@typescript-eslint/parser": "2.31.0",
    "clean-release": "2.8.0",
    "eslint": "7.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-plantain": "1.0.0",
    "jasmine": "3.5.0",
    "markdownlint-cli": "0.23.0",
    "no-unused-export": "1.10.3",
    "rimraf": "3.0.2",
    "ts-node": "8.10.1",
    "type-coverage": "2.5.0",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "minimist": "1",
    "pidusage": "2",
    "pretty-ms": "7",
    "tslib": "1"
  },
  "bin": {
    "clean-scripts": "bin/clean-scripts"
  }
}
