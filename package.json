    "lint:ts": "node dist/index.js lint.ts",
    "test[0]": "node dist/index.js test[0]"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/clean-scripts.git"
  },
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/clean-scripts/issues"
  },
  "typeCoverage": {
    "atLeast": 97
  },
  "homepage": "https://github.com/plantain-00/clean-scripts#readme",
  "devDependencies": {
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-conventional": "8.1.0",
    "@types/jasmine": "3.3.14",
    "@types/minimist": "1.2.0",
    "@types/node": "12.6.8",
    "@types/pidusage": "2.0.1",
    "@types/pretty-ms": "5.0.1",
    "clean-release": "2.7.0",
    "jasmine": "3.4.0",
    "markdownlint-cli": "0.17.0",
    "no-unused-export": "1.9.0",
    "rimraf": "2.6.3",
    "standard": "13.0.2",
    "tslint": "5.18.0",
    "tslint-config-standard": "8.0.1",
    "tslint-sonarts": "1.9.0",
    "type-coverage": "2.1.0",
    "typescript": "3.5.3"
  },
  "dependencies": {
    "minimist": "1.2.0",
    "pidusage": "^2.0.17",
    "pretty-ms": "5.0.0",
    "tslib": "1"
